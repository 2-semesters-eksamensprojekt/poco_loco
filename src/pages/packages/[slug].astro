import Form_information from "../../components/Form_information.astro";
import type { GetStaticPaths } from "astro";
export async function getStaticPaths() {
  const url = "https://mmrncfqaulcikmsvcajc.supabase.co/rest/v1/Packages";
  const key = "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6Im1tcm5jZnFhdWxjaWttc3ZjYWpjIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NjQ5MDk4NDksImV4cCI6MjA4MDQ4NTg0OX0.lCiHp8286uEb--d6v6H8AW5GG-NjQHHyJ3JfktN1F5s";

  const options = {
    headers: {
      apikey: key,
    },
  };

  const packagesRequest = await fetch(url, options);
  const packageData = await packagesRequest.json();

  return packageData.map((packages) => {
    return {
      params: { slug: packages.slug },
      props: { packages },
    };
  });
}

